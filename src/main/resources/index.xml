<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="reader" namespace="/reader" extends="struts-default">
        <action name="index" class="reader.IndexController">
            <result>/reader/index.jsp</result>
        </action>
        <action name="reader" class="reader.ReaderController">
            <result>/reader/reader.jsp</result>
        </action>
        <action name="profile" class="reader.ProfileController">
            <result>/reader/profile.jsp</result>
        </action>
    </package>
</struts>

