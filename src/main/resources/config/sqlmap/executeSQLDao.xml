<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ExecuteSQLDao">

    <statement id="executeNonQuery" parameterClass="java.lang.String" remapResults="true">
        $sqlString$
    </statement>

    <statement id="execute" parameterClass="java.lang.String" resultClass="java.util.HashMap" remapResults="true">
        $sqlString$
    </statement>

</sqlMap>